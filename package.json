{
  "name": "geojson-to-gml-3",
  "version": "2.0.2",
  "description": "a package to translate geojson to gml",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "src/index.ts",
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/index.d.ts",
    "LICENSE.md",
    "license/PolyForm-Noncommercial-1.0.0.md",
    "license/PolyForm-Noncommercial-1.0.0.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SKalt/geojson-to-gml-3.2.1.git"
  },
  "scripts": {
    "test": "vitest",
    "make-docs": "node make-docs.js",
    "build:cjs": "esbuild --format=cjs --out-extension:.js=.cjs --target=es2021 --tsconfig=./tsconfig.json --minify --sourcemap --outdir=dist ./src/index.ts",
    "build:mjs": "esbuild --format=esm --out-extension:.js=.mjs --target=es2021 --tsconfig=./tsconfig.json --minify --sourcemap --outdir=dist ./src/index.ts",
    "build": "pnpm build:cjs && pnpm build:mjs"
  },
  "keywords": [
    "geojson",
    "geoserver",
    "GIS",
    "GML",
    "xml"
  ],
  "author": "Steven Kalt",
  "license": "SEE LICENSE IN ./LICENSE.md",
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@vitest/coverage-v8": "^1.4.0",
    "cspell": "^8.6.1",
    "esbuild": "^0.20.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdoc-to-markdown": "8.0.1",
    "prettier": "3.2.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0",
    "xsd-schema-validator": "^0.9.0"
  }
}
